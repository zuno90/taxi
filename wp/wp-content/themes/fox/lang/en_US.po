msgid "FOX ADSENSE"
msgstr ""

#: admin/fox-adsense.php:14
msgid "THÊM"
msgstr "ADD"

#: admin/fox-adsense.php:15
msgid "VỊ TRÍ"
msgstr "LOCATION"

#: admin/fox-adsense.php:23
msgid "Bật quảng cáo Adsense trên Website"
msgstr "Enable Adsense ads on Website"

